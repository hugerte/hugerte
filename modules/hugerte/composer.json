{
	"name": "hugerte/hugerte",
	"version": "1.0.9",
	"description": "HugeRTE rich text editor",
	"license": "MIT",
	"keywords": [
		"wysiwyg",
		"hugerte",
		"richtext",
		"javascript",
		"html",
		"text",
		"rich editor",
		"rich text editor",
		"rte",
		"rich text",
		"contenteditable",
		"editing"
	],
	"homepage": "https://github.com/hugerte/hugerte",
	"type": "component",
	"funding": [
		{
			"type": "opencollective",
			"url": "https://opencollective.com/hugerte"
		}
	],
	"extra": {
		"component": {
			"scripts": [
				"tinymce.js",
				"plugins/*/plugin.js",
				"themes/*/theme.js",
				"models/*/model.js",
				"icons/*/icons.js"
			],
			"files": [
				"tinymce.min.js",
				"plugins/*/plugin.min.js",
				"themes/*/theme.min.js",
				"models/*/model.min.js",
				"skins/**",
				"icons/*/icons.min.js"
			]
		}
	},
	"archive": {
		"exclude": [
			"README.md",
			"bower.js",
			"package.json",
			".npmignore",
			"CHANGELOG.md"
		]
	}
}